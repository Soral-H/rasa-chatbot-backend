{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9.13"
  },
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "rasa run --enable-api --cors '*'"
}
